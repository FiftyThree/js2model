{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "arrayTest",
    "type": "object",
    "required": ["requiredArray"],
    "properties": {
        "requiredArray": {
            "type": "array",
            "items": {
                "type": "number",
                "minItems": 2
            }
        },
        "optionalStrings": {
            "type": "array",
            "items": {
                "type": "string",
                "minItems": 2
            }
        },
        "optionalBools": {
            "type": "array",
            "items": {
                "type": "boolean",
                "minItems": 2
            }
        },
        "optionalNumbers": {
            "type": "array",
            "items": {
                "type": "number",
                "minItems": 2
            }
        },
        "optionalInts": {
            "type": "array",
            "items": {
                "type": "integer",
                "minItems": 2
            }
        },
        "optionalEnums": {
            "type": "array",
            "items": {
                "enum": ["home", "work"],
                "minItems": 2
            }
        },
        "optionalObjects": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "foo": { "type": "integer" },
                    "bar": { "type": "boolean" }
                },
                "minItems": 2
            }
        }
    }
}
